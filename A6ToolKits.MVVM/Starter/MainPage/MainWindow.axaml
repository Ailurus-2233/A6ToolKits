<Window xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:mvvm="http://prismlibrary.com/"
             xmlns:mainPage="clr-namespace:A6ToolKits.MVVM.Starter.MainPage"
             mc:Ignorable="d"
             x:Class="A6ToolKits.MVVM.Starter.MainPage.MainWindow"
             x:DataType="mainPage:MainWindowModel"
             Title="{Binding Title}"
             mvvm:ViewModelLocator.AutoWireViewModel="True"
             Width="{Binding Width}" Height="{Binding Height}">
    <ContentControl mvvm:RegionManager.RegionName="DisplayRegion" />
</Window>
