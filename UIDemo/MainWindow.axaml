<Window x:Class="UIDemo.MainWindow"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:buttons="clr-namespace:A6ToolKits.UIPackage.Controls.Layout.Buttons;assembly=A6ToolKits.UIPackage"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:tab="clr-namespace:A6ToolKits.UIPackage.Controls.Layout.Tab;assembly=A6ToolKits.UIPackage"
        xmlns:uiDemo="clr-namespace:UIDemo"
        Title="UIDemo"
        d:DesignHeight="450"
        d:DesignWidth="800"
        x:DataType="uiDemo:MainWindow"
        mc:Ignorable="d">
    <DockPanel>
        <tab:TabHeader x:Name="TabHeader"
                       DockPanel.Dock="Top"
                       Height="30"
                       IconSize="15"
                       HorizontalAlignment="Left"
                       VerticalAlignment="Center"
                       DisplayType="Text"
                       PromptLinePosition="Bottom" />
        <tab:TabContainer x:Name="TabContainer" />
    </DockPanel>
</Window>
