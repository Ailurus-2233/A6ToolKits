<UserControl x:Class="A6ToolKits.Layout.Controls.PanelBar"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:tab="clr-namespace:A6ToolKits.UIPackage.Controls.Layout.Tab;assembly=A6ToolKits.UIPackage"
             d:DesignHeight="450"
             d:DesignWidth="800"
             Background="Fuchsia"
             mc:Ignorable="d">
    <DockPanel>
        <Grid DockPanel.Dock="Top">
            <tab:TabHeader x:Name="PanelTabHeader" />
        </Grid>
        <tab:TabBody x:Name="PanelTabBody" />
    </DockPanel>
</UserControl>